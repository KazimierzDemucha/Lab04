<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="175sp"
        android:layout_height="175sp"
        android:layout_marginTop="64dp"
        android:background="#292D37"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/B_1_1"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:onClick="ruch"
            android:textSize="28sp"
            app:iconTint="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="@+id/B_1_2"
            app:layout_constraintEnd_toStartOf="@+id/pion_lewo"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/B_1_2"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:onClick="ruch"
            android:textSize="28sp"
            app:layout_constraintBottom_toTopOf="@+id/poziom_gora"
            app:layout_constraintEnd_toStartOf="@+id/pion_prawo"
            app:layout_constraintHorizontal_bias="0.491"
            app:layout_constraintStart_toEndOf="@+id/pion_lewo"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/B_1_3"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:onClick="ruch"
            android:textSize="28sp"
            app:layout_constraintBottom_toBottomOf="@+id/B_1_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/pion_prawo"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/B_2_1"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:onClick="ruch"
            android:textSize="28sp"
            app:layout_constraintBottom_toTopOf="@+id/poziom_dol"
            app:layout_constraintEnd_toStartOf="@+id/pion_lewo"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/poziom_gora" />

        <Button
            android:id="@+id/B_2_2"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:onClick="ruch"
            android:textSize="28sp"
            app:layout_constraintBottom_toTopOf="@+id/poziom_dol"
            app:layout_constraintEnd_toStartOf="@+id/pion_prawo"
            app:layout_constraintStart_toEndOf="@+id/pion_lewo"
            app:layout_constraintTop_toBottomOf="@+id/poziom_gora" />

        <Button
            android:id="@+id/B_2_3"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:onClick="ruch"
            android:textSize="28sp"
            app:layout_constraintBottom_toTopOf="@+id/poziom_dol"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/pion_prawo"
            app:layout_constraintTop_toBottomOf="@+id/poziom_gora" />

        <Button
            android:id="@+id/B_3_1"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:onClick="ruch"
            android:textSize="28sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/pion_lewo"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/poziom_dol"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/B_3_2"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:onClick="ruch"
            android:textSize="28sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/pion_prawo"
            app:layout_constraintHorizontal_bias="0.483"
            app:layout_constraintStart_toEndOf="@+id/pion_lewo"
            app:layout_constraintTop_toBottomOf="@+id/poziom_dol"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/B_3_3"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:onClick="ruch"
            android:textSize="28sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/pion_prawo"
            app:layout_constraintTop_toBottomOf="@+id/poziom_dol"
            app:layout_constraintVertical_bias="1.0" />

        <View
            android:id="@+id/poziom_gora"
            android:layout_width="350dp"
            android:layout_height="4dp"
            android:layout_marginTop="114dp"
            android:background="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/poziom_dol"
            android:layout_width="350dp"
            android:layout_height="4dp"
            android:layout_marginBottom="114dp"
            android:background="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <View
            android:id="@+id/pion_lewo"
            android:layout_width="4dp"
            android:layout_height="350dp"
            android:layout_marginStart="114dp"
            android:background="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/pion_prawo"
            android:layout_width="4dp"
            android:layout_height="350dp"
            android:layout_marginEnd="114dp"
            android:background="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/restart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:onClick="restart"
        android:text="Restart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2" />

</androidx.constraintlayout.widget.ConstraintLayout>